{
  "name": "octokit-plugin-rename-branch",
  "version": "0.0.0-development",
  "description": "Octokit plugin to rename a branch in a github repository, its settings, and its code.",
  "scripts": {
    "build": "pika build",
    "lint": "prettier --check '{src,test}/**/*.{ts,md}' *.md package.json tsconfig.json",
    "lint:fix": "prettier --write '{src,test}/**/*.{ts,md}' *.md package.json tsconfig.json",
    "pretest": "npm run -s lint",
    "test": "jest --coverage"
  },
  "keywords": [
    "octokit",
    "plugin",
    "github",
    "branch"
  ],
  "author": "Gregor Martynus (https://github.com/gr2m)",
  "license": "MIT",
  "peerDependencies": {
    "@octokit/plugin-paginate-rest": ">= 2"
  },
  "devDependencies": {
    "@octokit/core": "^3.0.0",
    "@octokit/plugin-paginate-rest": "^2.13.3",
    "@pika/pack": "^0.5.0",
    "@pika/plugin-build-node": "^0.9.2",
    "@pika/plugin-build-web": "^0.9.2",
    "@pika/plugin-ts-standard-pkg": "^0.9.2",
    "@types/jest": "^27.0.0",
    "@types/node": "^16.0.0",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "semantic-release": "^17.0.0",
    "semantic-release-plugin-update-version-in-files": "^1.0.0",
    "ts-jest": "^26.1.0",
    "typescript": "^4.0.2"
  },
  "repository": "github:gr2m/octokit-plugin-rename-branch",
  "jest": {
    "preset": "ts-jest",
    "coverageThreshold": {
      "global": {
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
      }
    }
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-ts-standard-pkg"
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-build-web"
      ]
    ]
  },
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      "next",
      "next-major",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github",
      [
        "@semantic-release/npm",
        {
          "pkgRoot": "./pkg"
        }
      ],
      [
        "semantic-release-plugin-update-version-in-files",
        {
          "files": [
            "pkg/dist-web/*",
            "pkg/dist-node/*",
            "pkg/*/version.*"
          ]
        }
      ]
    ]
  }
}
